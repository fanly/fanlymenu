{
  "name": "vite-electron-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/source/main/entry.js",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "vite build --config ./config/main.vite.js",
    "postbuild:main": "cp ./src/main/entry.js ./dist/source/main/entry.js",
    "build:preload": "vite build --config ./config/preload.vite.js",
    "postbuild:preload": "cp ./src/preload/entry.js ./dist/source/preload/entry.js",
    "build:renderer": "vite build --config ./config/renderer.vite.js",
    "dev:renderer": "vite serve --config ./config/renderer.vite.js",
    "run:electron": "electron .",
    "electron-builder": "electron-builder build --config ./config/electron-builder.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "builtin-modules": "^3.1.0",
    "cross-env": "^7.0.2",
    "electron": "^11.0.2",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.0.1",
    "vite": "^1.0.0-rc.9"
  },
  "dependencies": {
    "electron-updater": "^4.3.5"
  }
}
